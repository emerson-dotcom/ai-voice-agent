# AI Voice Agent - Environment Configuration
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Retell AI Configuration
RETELL_API_KEY=your_retell_api_key_here
RETELL_WEBHOOK_SECRET=your_retell_webhook_secret_here

# OpenAI Configuration (Required for LLM-based data extraction)
OPENAI_API_KEY=your_openai_api_key_here

# Security Configuration
SECRET_KEY=your_super_secret_jwt_key_here_change_in_production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Admin User Configuration
ADMIN_EMAIL=admin@localhost
ADMIN_PASSWORD=admin123
ADMIN_NAME=System Administrator

# Environment
ENVIRONMENT=development

# API Configuration
DEBUG=true
LOG_LEVEL=INFO
LOG_TO_FILE=true

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=120
RATE_LIMIT_BURST=10

# Retell API Configuration
RETELL_API_TIMEOUT=30
RETELL_MAX_RETRIES=3

# CORS Configuration (comma-separated list, no spaces)
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000